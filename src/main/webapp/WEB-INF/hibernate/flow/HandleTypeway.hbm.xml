<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.shengdai.kpi.flow.valueobject.HandleTypeway" table="FLW_HANDLE_WAY">
		<cache usage="read-only"/>
		<id name="id" type="int">
			<column name="id" length="32" />
			<generator class="assigned" />
		</id>
		<property name="handleWayName" 
			column="name" 
			type="string"
		/>
		<property name="busiCode" 
			column="busi_code" 
			type="string"
		/>
		<property name="handleTypeClass" 
			column="handle_type_class" 
			type="string"
		/>
		<property name="beanName" 
			column="bean_name" 
			type="string"
		/>
		<property name="assClassName" 
			column="ASS_CLASS_NAME" 
			type="string"
		/>
		<property name="assBeanName" 
			column="ASS_BEAN_NAME" 
			type="string"
		/>
		<property name="pageRemark" 
			column="page_remark" 
			type="string"
		/>
		<property name="flgNeedHandler" 
			column="flg_need_handler" 
			type="int"
		/>		
		<property name="remark" 
			column="remark" 
			type="string"
		/>
		<property name="priority" 
			column="priority" 
			type="int"
		/>
		

	</class>
</hibernate-mapping>
